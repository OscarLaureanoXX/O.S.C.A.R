def int fibonacci(int limite) {
  var
    int inicio = 0, actual = 1, suma = 0; 
  
  for limite = read(int) : 15 {
    suma = inicio + actual;
    inicio = actual;
    actual = suma;
  }

  return suma;
}

main () {
  var
    int fib, limite;
  limite = read(int);
  fib = fibonacci(limite);
  print(fib);
}